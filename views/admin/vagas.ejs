        <%- include('header') %>
        
        <div class="lista">    
            <div class="titulo">
                <h2>Admin Vagas:</h2>
                <a href="/admin/vagas/nova">Nova vaga</a>
            </div>

            <br/><br/><br/>
    
            <div>           
                <ul>
                    <% vagas.forEach(vaga => { %>
                        <li>
                            <%- vaga.titulo %>
                            <a href="/admin/vagas/delete/<%- vaga.id %>">Excluir</a>
                            <a href="/admin/vagas/editar/<%- vaga.id %>">Editar</a>
                        </li>    
                    <% }) %> 
                </ul>
            </div>

            <br/><br/><br/><br/>

        <%- include('footer') %>